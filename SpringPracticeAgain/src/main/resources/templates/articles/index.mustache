{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
    {{#articleList}}
        <tr>
            <th>{{id}}</th>
            <th><a style="text-decoration:none;color:blue;" href="/articles/{{id}}">{{title}}</a></th>
            <th>{{content}}</th>
        </tr>
    {{/articleList}}
    </tbody>
</table>
<a href="/articles/new">새글작성</a>
{{>layouts/footer}}